{
  "name": "pipeline-react",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=6.x",
    "npm": ">=4.x"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "deploy": "yarn run clean && node -r babel-register bin/compile",
    "start": "nodemon -w server --exec 'node -r babel-register server' --ignore dist --ignore coverage --ignore tests --ignore src",
    "start:dev": "node -r babel-register server --ignore dist --ignore coverage",
    "start:prod": "node -r babel-register server",
    "lint": "eslint src tests --ext .js --ext .jsx",
    "lint:fix": "yarn run lint -- --fix",
    "test": "NODE_ENV=test node -r babel-register ./node_modules/karma/bin/karma start build/karma.conf.js && ./node_modules/.bin/mocha -r babel-register server/tests/**/*",
    "test:client": "NODE_ENV=test node -r babel-register ./node_modules/karma/bin/karma start build/karma.conf.js",
    "test:dev": "yarn run test -- --watch",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "firebase:deploy:functions": "firebase deploy --only functions",
    "firebase:deploy:rules": "firebase deploy --only database",
    "firebase:deploy:": "firebase deploy"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "license": "MIT",
